    "lodash": "~2.4.1",
    "prompt": "^0.2.13",
    "q": "^1.0.1",
    "tmp": "0.0.23",
    "xmlbuilder": "^2.2.1",
    "date-utils": "~1.2.16",
    "mkdirp": "~0.5.0"
  },
  "devDependencies": {
    "adm-zip": "^0.4.4",
    "chai": "^1.9.1",
    "chai-as-promised": "^4.1.1",
    "grunt": "^0.4.4",
    "grunt-concurrent": "^0.5.0",
    "grunt-contrib-jshint": "^0.9.2",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-mocha-test": "^0.10.0",
    "grunt-open": "^0.2.3",
    "grunt-shell-spawn": "^0.3.0",
    "jsrevival": "^0.2.8",
