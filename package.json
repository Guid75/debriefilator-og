{
  "private": true,
  "name": "debriefilator",
  "version": "0.0.0",
  "engines": {
    "node": "*"
  },
  "scripts": {
    "start": "APPJS=app.js && ./node_modules/forever/bin/forever start --watch --watchIgnore '*.log' -l forever.log --append -o app.log -e err.log $APPJS",
    "stop": "APPJS=app.js && ./node_modules/forever/bin/forever stop $APPJS"
  },
